<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

  <!-- Ontology Declaration -->
  <owl:Ontology rdf:about="http://www.example.org/ontologies/geologicalsurveys"/>
  
  <!-- Classes -->
  <owl:Class rdf:about="http://www.example.org/ontologies/geologicalsurveys#GeologicalTimescale"/>
  <owl:Class rdf:about="http://www.example.org/ontologies/geologicalsurveys#Location"/>
  <owl:Class rdf:about="http://www.example.org/ontologies/geologicalsurveys#Mineral"/>
  <owl:Class rdf:about="http://www.example.org/ontologies/geologicalsurveys#OreDeposit"/>
  <owl:Class rdf:about="http://www.example.org/ontologies/geologicalsurveys#Rock"/>
  <owl:Class rdf:about="http://www.example.org/ontologies/geologicalsurveys#Stratigraphy"/>
  <owl:Class rdf:about="http://www.example.org/ontologies/geologicalsurveys#Date"/>

  <!-- Object Properties -->
  <owl:ObjectProperty rdf:about="http://www.example.org/ontologies/geologicalsurveys#timescale">
    <rdfs:domain rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Rock"/>
    <rdfs:range rdf:resource="http://www.example.org/ontologies/geologicalsurveys#GeologicalTimescale"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.example.org/ontologies/geologicalsurveys#location">
    <rdfs:domain rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Rock"/>
    <rdfs:range rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Location"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.example.org/ontologies/geologicalsurveys#contains">
    <rdfs:domain rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Rock"/>
    <rdfs:range rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Mineral"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.example.org/ontologies/geologicalsurveys#contains">
    <rdfs:domain rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Mineral"/>
    <rdfs:range rdf:resource="http://www.example.org/ontologies/geologicalsurveys#OreDeposit"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.example.org/ontologies/geologicalsurveys#belongsTo">
    <rdfs:domain rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Stratigraphy"/>
    <rdfs:range rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Rock"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.example.org/ontologies/geologicalsurveys#foundIn">
    <rdfs:domain rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Rock"/>
    <rdfs:range rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Location"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.example.org/ontologies/geologicalsurveys#foundIn">
    <rdfs:domain rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Mineral"/>
    <rdfs:range rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Location"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.example.org/ontologies/geologicalsurveys#foundAt">
    <rdfs:domain rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Rock"/>
    <rdfs:range rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Date"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.example.org/ontologies/geologicalsurveys#foundAt">
    <rdfs:domain rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Mineral"/>
    <rdfs:range rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Date"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.example.org/ontologies/geologicalsurveys#foundAt">
    <rdfs:domain rdf:resource="http://www.example.org/ontologies/geologicalsurveys#OreDeposit"/>
    <rdfs:range rdf:resource="http://www.example.org/ontologies/geologicalsurveys#Date"/>
  </owl:ObjectProperty>

</rdf:RDF>